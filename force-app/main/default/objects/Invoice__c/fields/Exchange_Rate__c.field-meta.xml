<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Exchange_Rate__c</fullName>
    <description>Exchange Rate Based on currency of the TIG Entity chosen on project.</description>
    <externalId>false</externalId>
    <formula>(1/ CURRENCYRATE(TEXT(CurrencyIsoCode)) ) * ( CURRENCYRATE(TEXT(Project__r.TIG_Entity_Name__r.CurrencyIsoCode)) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Exchange Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>6</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
