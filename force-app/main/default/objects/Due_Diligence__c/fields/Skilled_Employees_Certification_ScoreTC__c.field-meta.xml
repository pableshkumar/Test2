<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Skilled_Employees_Certification_ScoreTC__c</fullName>
    <description>for Technical Capabilities</description>
    <externalId>false</externalId>
    <formula>CASE(TEXT(  Skilled_Employees_Certification_RatingTC__c ),
&apos;Exceed Expectations&apos;,  Skilled_Employees_Certification_WeightTC__c *1,
 &apos;Meet Expectations&apos;,Skilled_Employees_Certification_WeightTC__c *0.85,
&apos;Average&apos;,Skilled_Employees_Certification_WeightTC__c  *0.6,
&apos;High Risk&apos;,Skilled_Employees_Certification_WeightTC__c *0.25,
