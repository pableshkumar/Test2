<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_Signature_MSA_validation</fullName>
    <active>true</active>
    <description>Date of Signature (MSA) should not be a past date</description>
    <errorConditionFormula>Date_of_Signature_MSA__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date_of_Signature_MSA__c</errorDisplayField>
    <errorMessage>Date of Signature (MSA) should not be a past date.</errorMessage>
</ValidationRule>
