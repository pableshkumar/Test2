<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_ExpiryNDA_shouldnt_be_past_date</fullName>
    <active>true</active>
    <description>Date of Expiry (NDA) should not be a past date.</description>
    <errorConditionFormula>Date_of_Expiry_NDA__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date_of_Expiry_NDA__c</errorDisplayField>
    <errorMessage>Date of Expiry (NDA) should not be a past date.</errorMessage>
</ValidationRule>
