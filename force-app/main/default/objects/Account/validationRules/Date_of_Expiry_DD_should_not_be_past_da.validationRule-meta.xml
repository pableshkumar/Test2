<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_Expiry_DD_should_not_be_past_da</fullName>
    <active>true</active>
    <description>Date of Expiry (DD)</description>
    <errorConditionFormula>AND(
Date_of_Expiry_DD__c !=NULL,
Date_of_Expiry_DD__c &lt; TODAY())</errorConditionFormula>
    <errorDisplayField>Date_of_Expiry_DD__c</errorDisplayField>
    <errorMessage>Date of Expiry (DD) should not be a past date.</errorMessage>
</ValidationRule>
